{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug QuickStart",
            "request": "launch",
            "mainClass": "com.example.quickstart.QuickStartApplication",
            "projectName": "spring-boot-examples",
            "cwd": "${workspaceFolder}/spring-boot-examples"
        },
        {
            "type": "java",
            "name": "Debug Restful",
            "request": "launch",
            "mainClass": "com.example.restful.RestfulApplication",
            "projectName": "spring-boot-examples",
            "cwd": "${workspaceFolder}/spring-boot-examples"
        },
        {
            "type": "java",
            "name": "Debug JPA",
            "request": "launch",
            "mainClass": "com.example.jpa.JpaApplication",
            "projectName": "spring-boot-examples",
            "cwd": "${workspaceFolder}/spring-boot-examples"
        },
        {
            "type": "java",
            "name": "Spring Boot Maven (QuickStart)",
            "request": "launch",
            "mainClass": "com.example.quickstart.QuickStartApplication",
            "projectName": "spring-boot-examples",
            "cwd": "${workspaceFolder}/spring-boot-examples",
            "env": {
                "SPRING_OUTPUT_ANSI_ENABLED": "ALWAYS"
            },
            "vmArgs": "-Dspring-boot.run.fork=false"
        }
    ]
}
